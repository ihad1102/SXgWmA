<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@mipmap/background"
    >

    <android.support.v7.widget.Toolbar
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >
        <ImageView
            android:id="@+id/iv_return"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/fanhui"
            android:padding="@dimen/dp15"
            />
    </android.support.v7.widget.Toolbar>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="50px"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="@string/create_your_account"
                android:textSize="@dimen/sp25"
                android:layout_gravity="center_horizontal" />
            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="match_parent"
                android:layout_height="89px"
                android:layout_marginTop="39px"
                android:orientation="horizontal"
                android:gravity="center_vertical"
                android:layout_marginLeft="160px"
                android:layout_marginRight="160px"
                android:background="@drawable/login_line_bround"
                >
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="33px"
                    android:src="@mipmap/user"
                    />
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/shuxian"
                    android:layout_marginLeft="30px"
                    />
                <android.support.v7.widget.AppCompatEditText
                    android:id="@+id/et_user_name"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:paddingLeft="30px"
                    android:inputType="textPersonName"
                    android:hint="@string/user_name"
                    android:textSize="@dimen/sp14"
                    android:singleLine="true"
                    android:background="@color/transparent"
                    />
            </com.zhy.autolayout.AutoLinearLayout>
            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="match_parent"
                android:layout_height="89px"
                android:layout_marginTop="8px"
                android:orientation="horizontal"
                android:gravity="center_vertical"
                android:layout_marginLeft="160px"
                android:layout_marginRight="160px"
                android:background="@drawable/login_line_bround"
                >
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30px"
                    android:src="@mipmap/shouji"
                    />
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/shuxian"
                    android:layout_marginLeft="30px"
                    />
                <android.support.v7.widget.AppCompatEditText
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:paddingLeft="30px"
                    android:inputType="textPersonName"
                    android:hint="@string/phone_number"
                    android:textSize="@dimen/sp14"
                    android:singleLine="true"
                    android:background="@color/transparent"
                    />
            </com.zhy.autolayout.AutoLinearLayout>
            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="match_parent"
                android:layout_height="89px"
                android:layout_marginTop="8px"
                android:orientation="horizontal"
                android:gravity="center_vertical"
                android:layout_marginLeft="160px"
                android:layout_marginRight="160px"
                android:background="@drawable/login_line_bround"
                >
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="33px"
                    android:src="@mipmap/youxiang"
                    />
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/shuxian"
                    android:layout_marginLeft="30px"
                    />
                <android.support.v7.widget.AppCompatEditText
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:paddingLeft="30px"
                    android:inputType="textPersonName"
                    android:hint="@string/email"
                    android:textSize="@dimen/sp14"
                    android:singleLine="true"
                    android:background="@color/transparent"
                    />
            </com.zhy.autolayout.AutoLinearLayout>
            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="match_parent"
                android:layout_height="89px"
                android:layout_marginTop="8px"
                android:orientation="horizontal"
                android:gravity="center_vertical"
                android:layout_marginLeft="160px"
                android:layout_marginRight="160px"
                android:background="@drawable/login_line_bround"
                >
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="33px"
                    android:src="@mipmap/shenfenzheng"
                    />
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/shuxian"
                    android:layout_marginLeft="30px"
                    />
                <android.support.v7.widget.AppCompatEditText
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:paddingLeft="30px"
                    android:inputType="textPersonName"
                    android:hint="@string/id_cd"
                    android:textSize="@dimen/sp14"
                    android:singleLine="true"
                    android:background="@color/transparent"
                    />
            </com.zhy.autolayout.AutoLinearLayout>
            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="match_parent"
                android:layout_height="89px"
                android:layout_marginTop="8px"
                android:orientation="horizontal"
                android:gravity="center_vertical"
                android:layout_marginLeft="160px"
                android:layout_marginRight="160px"
                android:background="@drawable/login_line_bround"
                >
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="33px"
                    android:src="@mipmap/mima"
                    />
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/shuxian"
                    android:layout_marginLeft="30px"
                    />
                <android.support.v7.widget.AppCompatEditText
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:paddingLeft="30px"
                    android:inputType="textPersonName"
                    android:hint="@string/pwd"
                    android:textSize="@dimen/sp14"
                    android:singleLine="true"
                    android:background="@color/transparent"
                    />
            </com.zhy.autolayout.AutoLinearLayout>
            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="match_parent"
                android:layout_height="89px"
                android:layout_marginTop="8px"
                android:orientation="horizontal"
                android:gravity="center_vertical"
                android:layout_marginLeft="160px"
                android:layout_marginRight="160px"
                android:background="@drawable/login_line_bround"
                >
                <android.support.v7.widget.AppCompatEditText
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:paddingLeft="30px"
                    android:inputType="textPersonName"
                    android:hint="@string/input_code"
                    android:textSize="@dimen/sp14"
                    android:singleLine="true"
                    android:layout_weight="1"
                    android:background="@color/transparent"
                    />
                <com.zhy.autolayout.AutoLinearLayout
                    android:layout_width="wrap_content"
                    android:background="@mipmap/code"
                    android:gravity="center"
                    android:layout_height="match_parent">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/code"
                        android:textColor="@color/white"
                        />
                </com.zhy.autolayout.AutoLinearLayout>
            </com.zhy.autolayout.AutoLinearLayout>
            <android.support.v7.widget.AppCompatButton
                android:id="@+id/btn_login"
                android:layout_width="match_parent"
                android:layout_height="94px"
                android:layout_marginTop="52px"
                android:layout_marginLeft="160px"
                android:layout_marginRight="160px"
                android:text="@string/register"
                android:textSize="@dimen/sp18"
                android:textColor="@color/white"
                android:background="@drawable/register_immediately_bround"
                android:layout_gravity="center_horizontal"
                />
            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="55px"
                android:gravity="center_horizontal"
                android:orientation="horizontal"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/register_content1"
                    android:textSize="@dimen/sp12"
                    />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/register_content2"
                    android:textColor="@color/c1184ee"
                    android:textSize="@dimen/sp12"
                    />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/register_content3"
                    android:textSize="@dimen/sp12"
                    />
            </com.zhy.autolayout.AutoLinearLayout>
            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="25px"
                android:gravity="center_horizontal"
                android:orientation="horizontal"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/register_content4"
                    android:textSize="@dimen/sp12"
                    />
                <TextView
                    android:id="@+id/tv_login"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/register_content5"
                    android:textColor="@color/c1184ee"
                    android:textSize="@dimen/sp12"
                    />

            </com.zhy.autolayout.AutoLinearLayout>
</com.zhy.autolayout.AutoLinearLayout>