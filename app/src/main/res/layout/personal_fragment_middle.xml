<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_marginTop="107px"
    android:layout_height="wrap_content">
<com.zhy.autolayout.AutoLinearLayout
    android:id="@+id/ly_my_tream"
    android:layout_width="match_parent"
    android:layout_height="107px"
    android:orientation="horizontal"
    android:gravity="center_vertical"
    >
 <ImageView
     android:layout_width="wrap_content"
     android:layout_height="wrap_content"
     android:src="@mipmap/biaozhi"
     android:layout_marginLeft="33px"
     />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/my_tream"
        android:textSize="@dimen/sp18"
        android:layout_marginLeft="37px"
        />
</com.zhy.autolayout.AutoLinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/c999999"
        />
    <com.zhy.autolayout.AutoLinearLayout
        android:id="@+id/ly_my_performance"
        android:layout_width="match_parent"
        android:layout_height="107px"
        android:orientation="horizontal"
        android:gravity="center_vertical"
        >
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/biaozhi"
            android:layout_marginLeft="33px"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/my_performance"
            android:textSize="@dimen/sp18"
            android:layout_marginLeft="37px"
            />
    </com.zhy.autolayout.AutoLinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/c999999"
        />
    <com.zhy.autolayout.AutoLinearLayout
        android:id="@+id/ly_application_withdrawal"
        android:layout_width="match_parent"
        android:layout_height="107px"
        android:orientation="horizontal"
        android:gravity="center_vertical"
        >
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/biaozhi"
            android:layout_marginLeft="33px"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/application_withdrawal"
            android:textSize="@dimen/sp18"
            android:layout_marginLeft="37px"
            />
    </com.zhy.autolayout.AutoLinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/c999999"
        android:layout_marginBottom="107px"
        />
</com.zhy.autolayout.AutoLinearLayout>